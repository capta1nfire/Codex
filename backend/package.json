{
  "name": "backend",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "test:coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "test:ci": "NODE_OPTIONS=--experimental-vm-modules jest --ci --coverage",
    "test:unit": "NODE_OPTIONS=--experimental-vm-modules jest --testMatch='**/*.test.ts'",
    "test:integration": "NODE_OPTIONS=--experimental-vm-modules jest --testMatch='**/*.integration.test.ts'",
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "prisma:migrate": "prisma migrate dev",
    "prisma:generate": "prisma generate",
    "seed": "ts-node prisma/seed.ts",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\" \"**/*.json\" \"**/*.md\"",
    "cleanup-cache": "tsx src/scripts/cleanupApiKeyCache.ts",
    "benchmark-apikey": "tsx src/scripts/benchmarkApiKeyPerformance.ts",
    "validate-indexes": "tsx src/scripts/validateDatabaseIndexes.ts",
    "test-optimizations": "tsx src/scripts/testOptimizations.ts",
    "validate-all": "npm run validate-indexes && npm run benchmark-apikey",
    "validate-jules": "tsx src/scripts/validateJulesImplementation.ts",
    "make-admin": "tsx src/scripts/makeUserAdmin.ts",
    "make-superadmin": "tsx src/scripts/makeUserAdmin.ts",
    "list-users": "tsx src/scripts/makeUserAdmin.ts --list",
    "migrate-roles": "tsx src/scripts/migrateRoles.ts",
    "test-urls": "tsx src/scripts/testUrlValidation.ts",
    "codemod:add-js-ext": "jscodeshift -t ../scripts/add-js-extension.js . --parser=tsx --extensions=ts,tsx --ignore-pattern \"dist/*\" --ignore-pattern \"node_modules/*\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "^6.6.0",
    "@sentry/node": "^9.18.0",
    "axios": "^1.9.0",
    "bcrypt": "^5.1.1",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.14.1",
    "prom-client": "^15.1.3",
    "rate-limit-redis": "^4.2.0",
    "redis": "^4.7.0",
    "rust_generator": "file:../rust_generator/pkg",
    "sharp": "^0.34.1",
    "socket.io": "^4.8.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "undici": "^7.10.0",
    "uuid": "^9.0.1",
    "winston": "^3.17.0",
    "xss-clean": "^0.1.4",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@jest/globals": "^30.0.3",
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.20",
    "@types/express-rate-limit": "^5.1.3",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.13.14",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^10.0.0",
    "@types/winston": "^2.4.4",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.3.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.6",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "jest-mock-extended": "^4.0.0-beta1",
    "jscodeshift": "^17.3.0",
    "prisma": "^6.6.0",
    "supertest": "^7.1.0",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.30.1"
  }
}
